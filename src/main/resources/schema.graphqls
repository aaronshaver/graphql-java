type Query {
  bookById(id: ID): Book
}

type Mutation {
  createAuthor(input: AuthorInput): Author
}

type Book {
  id: ID
  name: String
  pageCount: Int
  author: Author
}

type Author {
  id: ID
  firstName: String
  lastName: String
}

input AuthorInput {
  firstName: String
  lastName: String
}